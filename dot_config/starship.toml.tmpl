"$schema" = 'https://starship.rs/config-schema.json'

[username]
show_always=true

[gcloud]
disabled = true

[kubernetes]
symbol = "󰠳 "
disabled = false
format = "[$symbol$context( )]($style) in "
[kubernetes.context_aliases]
"^gke_(?P<project>.{2,7}-(?P<tier>[\\w-]+))_(?P<region>[\\w-]+)_(?P<cluster>[\\w-]+)$" = "gke-$tier-$cluster"

[env_var.WSL_DISTRO_NAME]
disabled = false
symbol = ""
format = "using [$symbol $env_value]($style) "
style = "bright-purple"

