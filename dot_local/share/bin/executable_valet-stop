#!/usr/bin/env bash

root_services=(
    'php'
    'nginx'
    'dnsmasq'
)

user_services=(
    'mailhog'
    'mysql'
    'postgresql'
)

for service in "${root_services[@]}"; do
    sudo brew services stop "$service"
done

for service in "${user_services[@]}"; do
    brew services stop "$service"
done


